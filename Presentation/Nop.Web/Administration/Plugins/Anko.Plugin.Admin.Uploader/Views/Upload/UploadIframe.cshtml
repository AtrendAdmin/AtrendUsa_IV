@using System.Web.Optimization
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Upload IFrame</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="//s.echo.com/v0.2.4//dist/css/echox-bootstrap.min.css">
    <!--[if lt IE 9]><script src="//s.echo.com/v0.2.4/assets/js/html5shiv.js"></script><link href="//s.echo.com/v0.2.4/dist/css/echox-bootstrap-non-responsive.min.css" rel="stylesheet"></link><![endif]-->
    <!--[if true]><link href="//s.echo.com/v0.2.4/dist/css/echox-bootstrap-non-responsive.min.css" rel="stylesheet"></link><![endif]-->
</head>
<body>
    @using (Html.BeginForm("UploadFile", "Upload", FormMethod.Post, new { enctype = "multipart/form-data", role = "form" }))
    {
        <div id="fileDiv" class="form-group">
            <input type="file" name="inputFile" data-bind="value: $root.file, enable: $root.enableFile" />
            <input type="hidden" value="@Model.UploadTraceToken" data-bind="trace: $root.traceToken"/>
        </div>
        <button type="submit" id="uploadBtn" class="btn btn-default" data-bind="enable: $root.enableUpload">Upload</button>
    }
</body>
<!-- Latest compiled and minified JavaScript -->
<script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
<script src="//s.echo.com/v0.2.4//dist/js/knockout.min.js"></script>
@Scripts.Render("~/bundles/uploadFrameJs")
</html>
